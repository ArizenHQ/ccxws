{
  "name": "ccxws",
  "version": "0.47.0",
  "description": "Websocket client for 37 cryptocurrency exchanges",
  "keywords": [
    "bitmex",
    "binance",
    "coinbase",
    "cryptocurrency",
    "exchange",
    "websocket",
    "realtime"
  ],
  "author": "Brian Mancini <bmancini@gmail.com>",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "release": "release-it",
    "format:fix": "prettier --write  \"src/**\" \"__tests__/**\"",
    "format": "prettier --check \"src/**\" \"__tests__/**\"",
    "lint": "eslint src __tests__",
    "test": "nyc --reporter=lcov --reporter=text --extension=.ts mocha --require ts-node/register --recursive \"__tests__/**/*.spec.ts\""
  },
  "repository": "git+ssh://git@github.com/ArizenHQ/ccxws.git",
  "dependencies": {
    "crc": "^4.3.2",
    "decimal.js": "^10.2.0",
    "moment": "^2.26.0",
    "pusher-js": "^8.4.0-rc2",
    "semaphore": "^1.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.0",
    "@types/semaphore": "^1.1.1",
    "@types/sinon": "^17.0.3",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "chai": "^5.1.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^10.7.3",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "sinon": "^18.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
      "registry": "https://npm.pkg.github.com"
    }
}
